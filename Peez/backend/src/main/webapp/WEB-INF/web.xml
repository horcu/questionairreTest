<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <filter>
        <filter-name>ObjectifyFilter</filter-name>
        <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>ObjectifyFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>SystemServiceServlet</servlet-name>
        <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
        <init-param>
            <param-name>services</param-name>
            <param-value>com.horcu.apps.peez.backend.endpoints.misc.RegistrationEndpoint,
                com.horcu.apps.peez.backend.endpoints.misc.MessagingEndpoint,
                com.horcu.apps.peez.backend.endpoints.misc.UserSettingsEndpoint,
                com.horcu.apps.peez.backend.endpoints.gameboard.PieceEndpoint,
                com.horcu.apps.peez.backend.endpoints.gameboard.TileEndpoint,
                com.horcu.apps.peez.backend.endpoints.misc.GameEndpoint,
                com.horcu.apps.peez.backend.endpoints.gameboard.BoardEndpoint,
                com.horcu.apps.peez.backend.endpoints.gameboard.InvitationEndpoint,
                com.horcu.apps.peez.backend.endpoints.misc.PlayerEndpoint,
                com.horcu.apps.peez.backend.endpoints.gameboard.MoveEndpoint</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>SystemServiceServlet</servlet-name>
        <url-pattern>/_ah/spi/*</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>